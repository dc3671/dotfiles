{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": module,
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    },
  },
  "rules": {
    "no-use-before-define": [2, "nofunc"],
    "space-before-function-paren": [2, "never"]
  }
}
